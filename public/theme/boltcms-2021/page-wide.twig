{% extends 'page.twig' %}

{% block page_content %}
    {% block page_detail %}
        {{ parent() }}
    {% endblock %}

    {% if record.slug in ['community', 'contribute'] %}
        {% include 'partials/_contributors.twig' %}
    {% endif %}
{% endblock %}


{% block javascripts %}
    {% if record.slug in 'no-costs' %}
        {% include 'partials/_javascripts_pricing.twig' %}
    {% endif %}
{% endblock javascripts %}
